<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".CurrentOrderActivity" android:id="@+id/relativeLayout3">
    <TextView
            android:text="@string/subtotal_current_order"
            android:layout_width="82dp"
            android:layout_height="36dp" android:id="@+id/textView4"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintVertical_bias="0.556" app:layout_constraintHorizontal_bias="0.375"/>
    <TextView
            android:text="@string/sales_tax_current_order"
            android:layout_width="82dp"
            android:layout_height="36dp" android:id="@+id/textView5"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="452dp" android:layout_marginLeft="136dp" android:layout_marginStart="136dp"/>
    <TextView
            android:text="@string/total_current_order"
            android:layout_width="82dp"
            android:layout_height="36dp" android:id="@+id/textView6"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.596"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.375"/>
    <Button
            android:text="@string/place_current_order"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/placeCurrentOrderBtn"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="580dp" android:layout_marginLeft="148dp" android:layout_marginStart="148dp"/>
    <Button
            android:text="@string/remove_current_order"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/removeBtn"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="532dp" android:layout_marginLeft="164dp" android:layout_marginStart="164dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>
